VITE_POSTHOG_KEY=
# Optional, defaults to https://app.posthog.com
VITE_POSTHOG_HOST=
# Enable PostHog during `npm run dev` (true/false)
VITE_POSTHOG_ENABLE_DEV=false

# Official PostHogProvider alternative variable names (either works)
VITE_PUBLIC_POSTHOG_KEY=
VITE_PUBLIC_POSTHOG_HOST=

# UI links
# Override the GitHub repo link shown in the header (optional)
VITE_PUBLIC_GITHUB_REPO=
