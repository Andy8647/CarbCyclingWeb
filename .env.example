VITE_POSTHOG_KEY=
# Optional, defaults to https://app.posthog.com
VITE_POSTHOG_HOST=
# Enable PostHog during `npm run dev` (true/false)
VITE_POSTHOG_ENABLE_DEV=false

# Official PostHogProvider alternative variable names (either works)
VITE_PUBLIC_POSTHOG_KEY=
VITE_PUBLIC_POSTHOG_HOST=

# UI links
# Override the GitHub repo link shown in the header (optional)
VITE_PUBLIC_GITHUB_REPO=

# SEO / Meta
# Absolute site URL used for canonical/og:url (e.g., https://carb-cycling.pages.dev)
VITE_SITE_URL=
# Human-readable site name / <title>
VITE_SITE_NAME=Carb Cycling Planner
# Default meta description
VITE_META_DESCRIPTION=Open-source carb cycling planner that generates a personalized weekly macro plan with drag-and-drop, CSV/Markdown export, and PNG sharing. No login required.
# Default OG image URL (absolute or path like /og.png). Put an image at public/og.png or set a full URL.
VITE_OG_IMAGE=/og.png
# Optional: your Twitter handle (e.g., @yourhandle)
VITE_TWITTER_HANDLE=
